{
    "name": "teebbstudios/uploader-bundle",
    "description": "For symfony file upload.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teebb\\UploaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teebb\\UploaderBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "伟伟权",
            "email": "qww.zone@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5|>=8.0",
        "symfony/http-kernel": "^5.0|^6.0",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/form": "^5.0|^6.0",
        "symfony/validator": "^5.0|^6.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0|^3.0",
        "doctrine/orm": "^2.0",
        "symfony/proxy-manager-bridge": "^5.0|^6.0",
        "symfony/twig-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^5.0|^6.0",
        "symfony/css-selector": "^5.0|^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^5.0|^6.0"
    }
}
